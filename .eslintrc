{
    "env": {
        "browser": true,
        "node": true
    },

    "globals": {
        "React": true
    },

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "pragma": "React" // Pragma to use, default to "React"
    }
  },
    "plugins": [
        "react"
    ]
}
